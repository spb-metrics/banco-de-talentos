<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="br.gov.camara.negocio.comum.pojo.TipoMetaDado" table="TIPOMETADADO">
		<id name="identificador" column="ideTipoMetaDado" type="java.lang.Integer" >
			<generator class="increment" />
		</id>
		<property name="descricao" column="desTipoMetaDado" length="255" not-null="true" type="java.lang.String"/>
	</class>
	
</hibernate-mapping>
