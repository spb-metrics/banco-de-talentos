<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="br.gov.camara.negocio.comum.pojo.TipoHTML" table="TIPOHTML">
		<id name="identificador" column="ideTipoHTML" type="java.lang.Integer" >
			<generator class="increment" />
		</id>
		<property name="descricao" column="desTipoHTML" length="50" not-null="true" type="java.lang.String"/>
		<property name="multiplicidade" column="indMultiplicidade" length="1" not-null="true" type="java.lang.String"/>
	</class>
	
</hibernate-mapping>
