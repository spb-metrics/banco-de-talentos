<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.gov.camara.negocio.autenticacaoperfil.pojo.Grupo" table="GRUPO" lazy="true">
		<id name="codigo" column="codGrupo" type="java.lang.Integer" >
			<generator class="increment" />
		</id>
		<property name="descricao" column="desGrupo" not-null="true" type="java.lang.String"/>
	</class>
</hibernate-mapping>
